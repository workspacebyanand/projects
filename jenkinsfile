
//declarative pip[eline]
pipeline{
    agent any

    tools{
         maven "maven"
    }
    stages{
        stage("Build"){
            steps{
                echo "========build  mvn clean install======"
            }
         
        }
        stage("Test"){
            steps{
                echo "............testing............"
            }
        }
        stage("Deploy"){
            steps{
                echo ".............Deploy..........."
            }
        }

    }
    
}